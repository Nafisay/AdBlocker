{
    "manifest_version": 3,
    "name": "LinkedIn AdBlocker",
    "description": "Blocks LinkedIn ads",
    "version": "0.0.1",
    "author": "",
    "browser_action": {
        "default_title": "LinkedIn AdBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}